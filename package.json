        "globals": "^17.0.0",
        "html-webpack-plugin": "^5.6.5",
        "jsdom": "^27.4.0",
        "style-loader": "^4.0.0",
        "vitest": "^4.0.17",
        "webpack": "^5.104.1",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.3"
    }
